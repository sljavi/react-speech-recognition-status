{
  "name": "react-speech-recognition-status",
  "description": "React component useful to display the speech recognition status",
  "version": "1.2.8",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src ./tests",
    "build": "webpack -p --progress",
    "test": "NODE_ENV=testing jest --coverage --ci --runInBand && cat ./coverage/lcov.info | coveralls",
    "test:dev": "NODE_ENV=testing jest --watch",
    "profile": "webpack --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "4.x.x",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "6.x.x",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "eslint": "1.7.x",
    "eslint-plugin-babel": "2.x.x",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "webpack": "^2.6.1",
    "webpack-bundle-size-analyzer": "^2.7.0"
  },
  "main": "dist/index.js",
  "author": "Javier Perez",
  "bugs": {
    "url": "https://github.com/sljavi/react-speech-recognition-status/issues"
  },
  "homepage": "https://github.com/sljavi/react-speech-recognition-status",
  "repository": {
    "type": "git",
    "url": "https://github.com/sljavi/react-speech-recognition-status.git"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=15.x.x",
    "react-dom": ">=15.x.x",
    "prop-types": ">=15.x.x"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "testURL": "http://localhost"
  }
}
